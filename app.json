{
  "expo": {
    "name": "muvees-rn-expo",
    "slug": "muvees-rn-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/lib/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/lib/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sozonome.muvees.expo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/lib/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sozonome.muvees_exp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/lib/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d8a6c213-93c6-42dd-8447-d904919825a3"
      }
    }
  }
}
