{
  "cli": {
    "version": ">= 7.6.2",
    "requireCommit": true
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "EXPO_NO_DOTENV": "1",
        "APP_ENV": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "EXPO_NO_DOTENV": "1",
        "APP_ENV": "development"
      }
    },
    "production-apk": {
      "distribution": "internal",
      "channel": "production",
      "env": {
        "EXPO_NO_DOTENV": "1",
        "APP_ENV": "production"
      }
    },
    "production": {
      "channel": "production",
      "env": {
        "EXPO_NO_DOTENV": "1",
        "APP_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
