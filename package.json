{
  "name": "muvees-expo",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "upgrade:tamagui": "yarn up '*tamagui*'@latest '@tamagui/*'@latest",
    "upgrade:tamagui:canary": "yarn up '*tamagui*'@canary '@tamagui/*'@canary",
    "up-latest": "yarn upgrade-interactive",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "android:build-apk": "eas build --platform android --local --profile=preview",
    "android:release": "eas build --platform android --local --output=android-app-release.aab",
    "ios:preview": "eas build --platform ios --local --profile=preview",
    "ios:release": "eas build --platform android --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "yarn dlx @biomejs/biome lint ./src",
    "biome:apply": "yarn dlx @biomejs/biome check --apply ./src",
    "prepare": "husky"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@biomejs/biome": "^1.7.1",
    "@expo-google-fonts/nunito": "^0.2.3",
    "@expo/metro-runtime": "~3.1.3",
    "@react-navigation/native": "^6.1.17",
    "@shopify/flash-list": "1.6.3",
    "@tamagui/config": "^1.95.1",
    "@tamagui/lucide-icons": "^1.95.1",
    "@tamagui/metro-plugin": "^1.95.1",
    "axios": "^1.6.8",
    "babel-preset-expo": "^10.0.2",
    "expo": "~50.0.17",
    "expo-build-properties": "~0.11.1",
    "expo-font": "~11.10.3",
    "expo-linking": "~6.2.2",
    "expo-router": "~3.4.9",
    "expo-splash-screen": "~0.26.5",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.4",
    "expo-updates": "~0.24.12",
    "expo-web-browser": "~12.8.2",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "~2.0.4",
    "react-native": "0.73.6",
    "react-native-reanimated": "3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "^14.1.0",
    "react-native-web": "^0.19.10",
    "swr": "^2.2.5",
    "tamagui": "^1.95.1",
    "typescript": "^5.4.5",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@types/react": "~18.2.45",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2"
  },
  "private": true,
  "packageManager": "yarn@4.0.2"
}
