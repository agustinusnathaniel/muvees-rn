{
  "name": "muvees-expo",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "upgrade:tamagui": "yarn up '*tamagui*'@latest '@tamagui/*'@latest",
    "upgrade:tamagui:canary": "yarn up '*tamagui*'@canary '@tamagui/*'@canary",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "android:release-mode": "yarn android --variant release",
    "ios:release-mode": "yarn android --variant release",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "yarn dlx @biomejs/biome lint ./src",
    "toolchain:fix": "yarn dlx @biomejs/biome check --apply ./src",
    "prepare": "husky"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@biomejs/biome": "^1.5.3",
    "@expo/metro-runtime": "~3.1.3",
    "@react-navigation/native": "^6.0.2",
    "@tamagui/config": "^1.90.13",
    "@tamagui/lucide-icons": "^1.90.13",
    "@tamagui/metro-plugin": "^1.90.13",
    "axios": "^1.6.7",
    "babel-preset-expo": "^10.0.1",
    "expo": "~50.0.8",
    "expo-font": "~11.10.3",
    "expo-linking": "~6.2.1",
    "expo-router": "~3.4.7",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.3",
    "expo-web-browser": "~12.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "~2.0.4",
    "react-native": "0.73.4",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "^14.1.0",
    "react-native-web": "^0.19.9",
    "swr": "^2.2.5",
    "tamagui": "^1.90.13",
    "typescript": "^5.3.3",
    "zustand": "^4.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@types/react": "~18.2.14",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2"
  },
  "private": true,
  "packageManager": "yarn@4.0.2"
}
